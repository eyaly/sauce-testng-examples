<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Web Demo Suite" verbose="0" parallel="tests"
	thread-count="100">
	<listeners>
		<listener class-name="listeners.BaseTestListener"/>
	</listeners>


	<test name="Web Scenarios W3C Safari Mac Test" parallel="methods" enabled="true">
		<parameter name="API" value="sauce"></parameter>
		<parameter name="W3C" value="true"></parameter>
		<parameter name="browser" value="safari"></parameter>
		<parameter name="cap:browserVersion" value="13.1"></parameter>
		<parameter name="cap:platformName" value="macOS 10.15"></parameter>
		<parameter name="sauce:build" value="regression tests 18"></parameter>
		<groups>
			<run>
				<include name="@Web" />
			</run>
		</groups>
		<packages>
			<package name="tests.web"></package>
		</packages>
	</test>

